          <argument>
            <kind>1</kind>
            <string>///PHYSICS and CHECKING

scr_getinput();

switch (state) {
    case (turtleState.idle):
        scr_turtle_idle();
        break;
    case (turtleState.attack):
        scr_turtle_attack();
        break;
    case (turtleState.run):
        scr_turtle_run();
        break;
    case (turtleState.jump):
        scr_turtle_jump();
        break;
    case (turtleState.roll):
        scr_turtle_roll();
        break;
//    case (turtleState.defend):
//        scr_turtle_defend();
//        break;
}

//PHYSICS
if (moveY &lt; 10) //roi xuong voi toc do tang dan va toi da la 9
{
    moveY = min(moveY + grav, 9);
}

//COLLISION CHECK
if (place_meeting(x, y + moveY, obj_ground))
{
        while (not place_meeting(x, y + sign(moveY), obj_ground)) //khi khong sap va cham mat dat thi chuyen dong tiep
        {
            y += sign(moveY);                  
        }
            
    moveY = 0; // khi va cham thi dung yen
}

if (place_meeting(x + moveX, y - 2, obj_ground))
{
        while (not place_meeting(x + sign(moveX), y - 2, obj_ground))
        {
            x += sign(moveX);
        }
                
    moveX = 0;
}


// Apply Movement
x += moveX;
y += moveY;

// Sprite controller
if (pressLeft + pressRight != 0) { //neu nhu dang an 1 trong 2 phim
    image_xscale = sign(pressRight - pressLeft); //mac dinh quay ve ben phai
    global.left = sign(pressLeft - pressRight);
}


</string>
